{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "ordered-imports": [false],
    "variable-name": [true, "check-format", "allow-pascal-case"],
    "import-name": [false],
    "no-console": [true, "warning"],
    "object-literal-sort-keys": [false],
    "noImplicitAny": false
  }
}
